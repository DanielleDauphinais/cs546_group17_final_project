<h1>Owned Libraries</h1>
    {{#each ownedLibs}}
    <div class="library">
      <h2>{{this.name}}</h2>
      <p>Fullness: {{this.fullnessRating}}</p>
      <a href="/libraries/{{this._id}}">Expand</a>
    </div>
    {{/each}}

<h1>Favorited Libraries</h1>
    {{#each favLibs}}
    <div class="library">
      <h2>{{this.name}}</h2>
      <p>Fullness: {{this.fullnessRating}}</p>
      <button>Unfavorite</button>
      <a href="/libraries/{{this._id}}">Expand</a>
    </div>
    {{/each}}